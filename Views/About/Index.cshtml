@using TravelTripProject.Models.Classes;
@model List<About>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="content">
	<div class="wrap">
		<div class="about">
			<h3>About</h3>
			@foreach(var about in Model)
			{
				<img src="@about.PhotoUrl" alt="" />
				<p>
					@about.Description
				</p>
			}
		</div>
	</div>
</div>